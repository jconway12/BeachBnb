      window.minusURL = "<%= image_url('minus.png') %>";
      window.plusURL = "<%= image_url('plus.png') %>";
      window.checkURL = "<%= image_url('checkmark.png') %>";
      window.linkedinURL = "<%= image_url('linkedin.png') %>";
      window.angellistURL = "<%= image_url('angellist.png') %>";

    </script>

    <link href="https://fonts.googleapis.com/css?family=Lato&display=swap" rel="stylesheet">
  </head>

  <body>
    <%= yield %>
